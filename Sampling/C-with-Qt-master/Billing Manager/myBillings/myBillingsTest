#!/bin/sh
prog=./myBillings
echo "************************************"
echo "Tests parsing"
echo "************************************"
echo ""
rm mybillings.dat
echo "************************************"
$prog -add -n "John Smith" -d "03/22/11" -s "10:11" -e "10:12"
echo "************************************"
echo "Testing incorrect -add command"
echo "************************************"
$prog -ads -n "John Tyler" -d "03/23/11" -s "10:11" -e "10:12"
echo "************************************"
echo "Testing incorrect -n command"
echo "************************************"
$prog -add -4 "amos west" -d "03/23/11" -s "10:13" -e "10:14"
echo "************************************"
echo "Testing incorrect -d command"
echo "************************************"
$prog -add -n "amos west" -g "03/23/11" -s "10:13" -e "10:14"
echo "************************************"
echo "Testing incorrect -date month command"
echo "************************************"
$prog -add -n "amos west" -d "13/23/11" -s "10:13" -e "10:14"
echo "************************************"
echo "Testing incorrect -date day command"
echo "************************************"
$prog -add -n "amos west" -d "13/00/11" -s "10:13" -e "10:14"
echo "************************************"
echo "Testing incorrect -date year command"
echo "************************************"
$prog -add -n "amos west" -d "12/23/14" -s "10:13" -e "10:14"
echo "************************************"
echo "Testing incorrect -time1 hour command"
echo "************************************"
$prog -add -n "amos west" -d "12/23/11" -s "25:13" -e "10:14"
echo "************************************"
echo "Testing incorrect -time1 minute command"
echo "************************************"
$prog -add -n "amos west" -d "12/23/11" -s "25:asd" -e "10:14"
echo "************************************"
echo "Testing incorrect -e command"
echo "************************************"
$prog -add -n "amos west" -d "12/23/11" -s "25:13" -g "10:14"
echo "************************************"
echo "Testing incorrect -time2 hour command"
echo "************************************"
$prog -add -n "amos west" -d "12/23/11" -s "25:13" -e "ab:14"
echo "************************************"
echo "Testing incorrect -time2 minute command"
echo "************************************"
$prog -add -n "amos west" -d "12/23/11" -s "25:13" -e "10:66"


echo "************************************"
echo "Testing  correct delete"
echo "************************************"
echo ""
rm mybillings.dat
$prog -add -n "0" -d "03/21/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -add -n "1" -d "03/22/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -add -n "2" -d "03/23/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -add -n "3" -d "03/24/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -add -n "4" -d "03/25/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -add -n "5" -d "03/26/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -del "1004"
echo "************************************"
echo "Incorrect confirmation number"
echo "************************************"
$prog -del "1111"


echo "************************************"
echo "Testing DayPrint Out"
echo "************************************"
echo ""
rm mybillings.dat
$prog -add -n "0" -d "03/21/11" -s "11:11" -e "11:12"
echo "************************************"
$prog -add -n "1" -d "03/22/11" -s "12:11" -e "12:12"
echo "************************************"
$prog -add -n "2" -d "03/21/11" -s "13:11" -e "13:12"
echo "************************************"
$prog -add -n "3" -d "03/22/11" -s "14:11" -e "14:12"
echo "************************************"
$prog -add -n "4" -d "03/21/11" -s "15:11" -e "15:12"
echo "************************************"
$prog -add -n "5" -d "03/22/11" -s "16:11" -e "16:12"
echo "************************************"
$prog -add -n "6" -d "03/21/11" -s "17:11" -e "17:12"
echo "************************************"
$prog -add -n "7" -d "03/22/11" -s "18:11" -e "18:12"
echo "************************************"
$prog -add -n "8" -d "03/21/11" -s "19:11" -e "19:12"
echo "************************************"
$prog -list -d "03/21/11"


echo "************************************"
echo "Testing Name Print"
echo "************************************"
echo ""
rm mybillings.dat
$prog -add -n "jimmy" -d "03/21/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -add -n "jimmy" -d "03/22/11" -s "11:11" -e "11:12"
echo "************************************"
$prog -add -n "JIMMY" -d "03/21/11" -s "12:11" -e "12:12"
echo "************************************"
$prog -add -n "JiMmY" -d "03/22/11" -s "13:11" -e "13:12"
echo "************************************"
$prog -add -n "JimMY" -d "03/21/11" -s "14:11" -e "14:12"
echo "************************************"
$prog -add -n "5" -d "03/22/11" -s "10:11" -e "15:12"
echo "************************************"
$prog -add -n "Dewey, Cheetam, and Bill" -d "03/21/11" -s "16:11" -e "16:12"
echo "************************************"
$prog -add -n "7" -d "03/22/11" -s "17:11" -e "17:12"
echo "************************************"
$prog -add -n "8" -d "03/21/11" -s "18:11" -e "18:12"
echo "************************************"
$prog -list -n "jimmy"


echo "************************************"
echo "Test overlaping time"
echo "************************************"
echo ""
rm mybillings.dat
$prog -add -n "jimmy" -d "03/22/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -add -n "jimmy" -d "03/22/11" -s "10:09" -e "10:10"
echo "************************************"
$prog -add -n "JIMMY" -d "03/22/11" -s "10:17" -e "10:18"
echo "************************************"
$prog -add -n "Jimmy and tyler" -d "03/22/11" -s "10:25" -e "10:30"
echo "************************************"
$prog -add -n "JimMY" -d "03/22/11" -s "10:31" -e "10:33"
echo "************************************"
echo "Test1"
echo "************************************"
$prog -add -n "Jimmy" -d "03/22/11" -s "10:30" -e "10:33"
echo "************************************"
echo "Test2"
echo "************************************"
$prog -add -n "Jimmy" -d "03/22/11" -s "10:26" -e "10:27"
echo "************************************"
echo "Test3"
echo "************************************"
$prog -add -n "Jimmy" -d "03/22/11" -s "10:32" -e "10:35"
echo "************************************"
echo "Test4"
echo "************************************"
$prog -add -n "Jimmy" -d "03/22/11" -s "10:30" -e "10:34"
echo "************************************"

rm mybillings.dat
echo "************************************"
echo "Test total"
echo "************************************"
echo ""
$prog -add -n "jimmy" -d "03/22/11" -s "10:11" -e "10:12"
echo "************************************"
$prog -add -n "jimmy" -d "03/22/11" -s "10:09" -e "10:10"
echo "************************************"
$prog -add -n "JIMMY" -d "03/22/11" -s "10:17" -e "10:18"
echo "************************************"
$prog -add -n "Jimmy and tyler" -d "03/22/11" -s "10:25" -e "10:30"
echo "************************************"
$prog -add -n "JimMY" -d "03/22/11" -s "10:31" -e "10:33"
echo "************************************"
$prog -total







